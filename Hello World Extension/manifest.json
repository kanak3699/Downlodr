{
    "manifest_version": 2,
    "name": "Downlodr",
    "description": "Extension to download videos and audios ",
    "version": "1.0",
    "icons": {
        "128": "128.png"
    },
    "browser_action": {
        "default title": "Downlodr",
        "default_icon": "logo.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "downloads",
        "activeTab",
        "storage",
        "declarativeContent"
    ],
    "background" : {
        "scripts":["background.js","jquery-3.4.1.min.js"],
        "persistent":false
    }
}